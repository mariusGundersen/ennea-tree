{
  "name": "ennea-tree",
  "version": "2.1.0",
  "description": "Immutable tilemap quad-tree with support for mega-tiles",
  "main": "js/main.js",
  "types": "es/main.d.ts",
  "module": "es/main.js",
  "scripts": {
    "clean": "rm -rf ./es ./js",
    "test": "ava",
    "tsc": "tsc",
    "compile": "npm run tsc && npm run babel",
    "babel": "babel --source-maps -d js es",
    "watch": "tsc -w & ava -w",
    "prepublish": "npm run clean && npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mariusGundersen/ennea-tree.git"
  },
  "keywords": [
    "immutable",
    "tilemap",
    "quad-tree"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/ennea-tree/issues"
  },
  "homepage": "https://github.com/mariusGundersen/ennea-tree#readme",
  "devDependencies": {
    "@types/node": "^7.0.65",
    "ava": "^0.19.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "typescript": "^2.9.1"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "es/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "peerDependencies": {
    "babel-runtime": "^6.22.0"
  }
}
