{
  "name": "ennea-tree",
  "version": "2.0.4",
  "description": "Immutable tilemap quad-tree with support for mega-tiles",
  "main": "dist/main.js",
  "types": "lib/main.d.ts",
  "module": "lib/main.js",
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "test": "ava",
    "tsc": "tsc",
    "compile": "npm run tsc && npm run babel",
    "babel": "babel --source-maps -d dist lib",
    "watch": "tsc -w & ava -w",
    "prepublish": "npm run clean && npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mariusGundersen/ennea-tree.git"
  },
  "keywords": [
    "immutable",
    "tilemap",
    "quad-tree"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/ennea-tree/issues"
  },
  "homepage": "https://github.com/mariusGundersen/ennea-tree#readme",
  "devDependencies": {
    "@types/node": "^7.0.5",
    "ava": "^0.16.0",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "typescript": "^2.1.5"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "lib/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "peerDependencies": {
    "babel-runtime": "^6.22.0"
  }
}
